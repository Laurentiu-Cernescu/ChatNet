<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <!--<system.web>
    <compilation debug="true" />
  </system.web>-->

  <connectionStrings>
    <add name="ChatNetEntities" connectionString="metadata=res://*/DataModel.csdl|res://*/DataModel.ssdl|res://*/DataModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=asus;initial catalog=ChatNet;integrated security=True;pooling=False;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>

  <system.serviceModel>
    <services>
      <service name="WcfChatService.ChatService"
               behaviorConfiguration="WcfChatService.MessageBehavior">
        <endpoint address ="net.tcp://localhost:9191/chat"
                  binding="netTcpBinding"
                  contract="WcfChatService.IChatService">
          
          <!--<identity>
            <dns value="localhost"/>
          </identity>-->

        </endpoint>

        <endpoint address="mex"
                  binding="mexTcpBinding"
                  contract="IMetadataExchange"/>
      </service>
    </services>

    <behaviors>
      <serviceBehaviors>
        <behavior name="WcfChatService.MessageBehavior">
          <serviceMetadata />
          <!--<serviceDebug includeExceptionDetailInFaults="False" />-->
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
</configuration>